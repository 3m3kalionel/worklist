{
  "name": "worklist",
  "version": "1.0.0",
  "description": "An application for creating todo lists and tasks for collaboration",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect src/index.js",
    "playground": "graphql playground --dotenv .env",
    "dev": "npm-run-all --parallel debug playground",
    "deploy": "prisma deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3m3kalionel/worklist.git"
  },
  "keywords": [
    "todo",
    "tasks",
    "todolists"
  ],
  "author": "3m3kalionel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/3m3kalionel/worklist/issues"
  },
  "homepage": "https://github.com/3m3kalionel/worklist#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "graphql": "^14.3.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.34.0",
    "prisma-binding": "^2.3.15"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "nodemon": "^1.19.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  }
}
