{
  "name": "worklist",
  "version": "1.0.0",
  "description": "An application for creating todo lists and tasks for collaboration",
  "main": "index.js",
  "scripts": {
    "server:start": "nodemon -e js,graphql -x node src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect src/index.js",
    "client:dev": "next -p 7777 ./client",
    "build": "next build ./client",
    "client:start": "next ./client",
    "playground": "graphql playground --dotenv .env",
    "server:dev": "npm-run-all --parallel debug playground",
    "deploy": "prisma deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3m3kalionel/worklist.git"
  },
  "keywords": [
    "todo",
    "tasks",
    "todolists"
  ],
  "author": "3m3kalionel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/3m3kalionel/worklist/issues"
  },
  "homepage": "https://github.com/3m3kalionel/worklist#readme",
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.2",
    "babel-plugin-styled-components": "^1.7.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "graphql": "^14.0.2",
    "graphql-cli": "^3.0.11",
    "graphql-tag": "^2.9.2",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.0.2",
    "next-with-apollo": "^3.4.0",
    "nodemailer": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.34.0",
    "prisma-binding": "^2.3.15",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.2",
    "react-dom": "^16.8.6",
    "react-tooltip": "^3.10.0",
    "shortid": "^2.2.14",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "nodemon": "^1.19.1"
  }
}
