{
  "env": {
    "development": {
      "presets": [
        "next/babel",
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": ["last 2 Chrome versions"]
            }
          }
        ]
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          },
          "@babel/plugin-proposal-object-rest-spread"
        ]
      ]
    },
    "production": {
      "presets": [
        "next/babel",
        "@babel/preset-env",
        {
          "targets": {
            "browsers": ["last 2 Chrome versions"]
          }
        }
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          },
          "@babel/plugin-proposal-object-rest-spread"
        ]
      ]
    },
    "test": {
      "presets": [
        "next/babel",
        "@babel/preset-env",
        {
          "targets": {
            "browsers": ["last 2 Chrome versions"]
          },
          "modules": "commonjs"
        }
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          },
          "@babel/plugin-proposal-object-rest-spread"
        ]
      ]
    }
  }
}
